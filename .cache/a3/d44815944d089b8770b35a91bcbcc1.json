is.html5QrCode.stop().then(ignore => {\r\n            alert('Scanning stopped')\r\n        }).catch(err => {\r\n            alert('Error when stopping')\r\n        });\r\n    }\r\n\r\n    makeRequest(){\r\n        fetch('https://7dwxc.sse.codesandbox.io/qr-scanner', {\r\n            method: 'post',\r\n        }).then(function(response) {\r\n            return response.json();\r\n        }).then(function(data) {\r\n            console.log(data);\r\n        });\r\n    }\r\n}\r\n\r\nconst app = new App();\r\n"},"lineCount":null}},"error":null,"hash":"9f301975970c8b530e27cfff3c26accd","cacheData":{"env":{}}}